{
  "manifest_version": 2,
  "name": "鼠标效果",
  "version": "0.0.1",
  "description": "鼠标效果",
  "author": "vist",
  "icons": {
    "16": "img/icon.png",
    "48": "img/icon.png",
    "128": "img/icon.png"
  },
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_title": "鼠标效果",
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "matches": ["*://*/*"],
    "js": ["index.js"]
  }],
  "homepage_url": "https://github.com/bestvist/crx_mouse_click",
  "permissions": [  "tabs"]
}